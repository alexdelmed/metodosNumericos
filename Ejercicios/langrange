from sympy import *
import matplotlib.pyplot as plt
import numpy 
x = [0, 0.33,0.66,1]
y = [1,1.391,1.935,2.718]


pL = ''
for i in range(len(x)):
    pL += str(y[i]) 
    for j in range(len(x)):
        if(j == k):
            continue
        pL += '(x - %f)*'%(x[j])
        Lxk *= x[k] - x[j]
    pL = pL [:-1] + '/%f) +'%(Lxk)
pL = pL[:-1]
expr = sumpify(pL)
print(pL)    

plt.plot(x,y,'ro')
